class GfG
{
    /*You are required to complete this method*/
    boolean isToepliz(int matrix[][], int n, int m)
    {
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                if(i>0 && j>0 && matrix[i][j]!=matrix[i-1][j-1]){
                    return false;
                }
            }
        }
        return true;
    }
}
