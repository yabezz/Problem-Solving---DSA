class Solution {
    
    public long firstIndex(long arr[], long n)
    {
        // Your code goes here
       long sum=0;
       for(int i=0;i<n;i++){
           sum+=arr[i];
       }
       if(sum==0){
           return -1;
       }
       else{
           return n-sum;
       }
       
    }
}
