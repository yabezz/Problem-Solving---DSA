class GfG
{
    public int[] formatArray(int[] a,int n)
        {
        // add code here.
        int temp;
        for(int i = 0; i < n; i++){
            if(i % 2 == 0 && i < n-1){
                if(a[i] > a[i+1]){
                    temp = a[i];
                    a[i] = a[i+1];
                    a[i+1] = temp;   
                }
            }
        }
        return a;
    }
}
