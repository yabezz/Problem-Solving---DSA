Elements in the Range


class Solution
{
    boolean check_elements(int arr[], int n, int A, int B)
    {
        for(int i=A;i<=B;i++){
            boolean flag = false;
            for(int j=0;j<n;j++){
                if(i == arr[j]){
                    flag = true;
                }
            }
            if(!flag)
            {
                return false;
            }
        }
        return true;
    }
}
