class Complete{
    // Function for finding maximum and value pair
    public static int multiply (int arr[], int n) {
        //Complete the function
        int sum=0;
        for(int i=0;i<n/2;i++){
            sum=sum+arr[i];
        }
        int Sum=0;
        for(int i=n/2;i<n;i++){
            Sum=Sum+arr[i];
        }
        return sum*Sum;
    }
    
    
}
