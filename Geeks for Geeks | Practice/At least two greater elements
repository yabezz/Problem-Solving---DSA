At least two greater elements

class Solution {
    public long[] findElements( long a[], long n)
    {
        // Your code goes here
        Arrays.sort(a);
        int index = 0;
        long b[] = new long[a.length-2];
        for(int i=0;i<a.length-2;i++){
            b[index++] = a[i];
        }
        return b;
    }
}
