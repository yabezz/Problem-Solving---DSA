Value equal to index value

class Solution {
    ArrayList<Integer> valueEqualToIndex(int arr[], int n) {
        ArrayList<Integer> al = new ArrayList<>();
        for(int i=0;i<n;i++)
        {
            if(arr[i] == i+1){
                al.add(arr[i]);
            }
        }
        
        return al;
    }
}
