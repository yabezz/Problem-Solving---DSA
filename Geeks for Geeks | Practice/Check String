class Sol
{
    Boolean check (String s)
    {
        // your code here
        int count=0;
        for(int i=0;i<s.length();i++)
        {
            if(s.charAt(0)==s.charAt(i))
            {
            count++;
            }
        }
        if(count==s.length())
        {
            return true;
        }
        else{
            return false;
        }
    }
}
